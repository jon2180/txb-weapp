/* pages/publish/publish.wxss */
page {
  background-color: #fff;
}

.box {
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.publish-container{
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: space-around;
}

.pub-commodity {
  width: 250rpx; 
}

.pub-commodity-icon {
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
}

.pub-commodity-label {
  display: block;
  width: 100%;
  text-align: center;
}